<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


	<bean id="r" class="com.techment.modal.Restraunt">
		<property name="restrauntName" value="A@B"></property>
		<property name="hotDrink" ref="c"></property>
		<property name="employeeNames">
			<list>
				<value>sachin</value>
				<value>amit</value>
				<value>rahul</value>
			</list>
		</property>
		<property name="softDrink" ref="ma"></property>
	</bean>

	<bean id="t" class="com.techment.modal.Tea">
		<property name="price" value="10"></property>
	</bean>


	<bean id="c" class="com.techment.modal.Coffee">

	</bean>

	<bean id="m" class="com.techment.modal.Milk">

	</bean>

	<bean id="p" class="com.techment.modal.Pepsi">

	</bean>

	<bean id="ma" class="com.techment.modal.Mazza">

	</bean>


</beans> 