<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="u" class="com.techment.modal.University" autowire="byName">
<property name="noOfStudents" value="100" ></property>
<property name="department" ref="departme"></property>
 </bean>

<bean id="department" class="com.techment.modal.Department">
<property name="hodName" value="rk"></property>
</bean>


<bean id="departme" class="com.techment.modal.Department">
<property name="hodName" value="rk2"></property>
</bean>

</beans> 